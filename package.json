{
  "author": "",
  "name": "palavara-front-v1",
  "version": "0.0.1",
  "description": "Elm 0.19 starter with Webpack 4 hot-reloading",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonh1000/elm-webpack-starter.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node scripts/start.js",
    "package": "elm-package",
    "make": "elm-make",
    "repl": "elm-repl",
    "reactor": "elm-reactor",
    "test": " ./node_modules/elm-test/bin/elm-test",
    "dev": "webpack-dev-server --hot --colors --port 3000",
    "prod": "webpack -p",
    "analyse": "elm-analyse -s -p 3001 -o",
    "deploy": "npm run build && scripts/deploy.sh",
    "deploy-production": "npm run build && scripts/deploy-prod.sh"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.7",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "elm-analyse": "^0.16.5",
    "elm-hot-webpack-loader": "^1.1.5",
    "elm-test": "^0.19.1-1",
    "elm-webpack-loader": "^6.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^10.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^2.3.1",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "elm": "^0.19.1-3"
  },
  "prettier": {
    "tabWidth": 4
  }
}
